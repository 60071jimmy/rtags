TEMPLATE = lib
TARGET = leveldb
DESTDIR = ..

OBJECTS_DIR = .obj_ldb

CONFIG -= qt
CONFIG += warn_off static

linux-* {
  QMAKE_CFLAGS += -DOS_LINUX
  QMAKE_CXXFLAGS += -DOS_LINUX
}

freebsd-* {
  QMAKE_CFLAGS += -DOS_FREEBSD
  QMAKE_CXXFLAGS += -DOS_FREEBSD
}

macx-* {
  QMAKE_CFLAGS += -DOS_MACOSX
  QMAKE_CXXFLAGS += -DOS_MACOSX
}

QMAKE_CFLAGS += -Isrc -Isrc/include -fno-builtin-memcmp -DLEVELDB_PLATFORM_POSIX -pthread -O2 -DNDEBUG
QMAKE_CXXFLAGS += -Isrc -Isrc/include -fno-builtin-memcmp -DLEVELDB_PLATFORM_POSIX -pthread -O2 -DNDEBUG

SOURCES += \
